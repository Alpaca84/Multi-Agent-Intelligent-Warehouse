{
  "dashboard": {
    "id": null,
    "title": "Warehouse Safety & Compliance",
    "tags": ["warehouse", "safety", "compliance"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Safety Incidents by Type",
        "type": "graph",
        "targets": [
          {
            "expr": "increase(warehouse_safety_incidents_total[1h])",
            "legendFormat": "{{incident_type}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Incidents/hour",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Safety Score Trend",
        "type": "graph",
        "targets": [
          {
            "expr": "warehouse_safety_score",
            "legendFormat": "Safety Score",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Score",
            "min": 0,
            "max": 100
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Compliance Checklist Status",
        "type": "stat",
        "targets": [
          {
            "expr": "warehouse_compliance_checks_passed",
            "legendFormat": "Passed",
            "refId": "A"
          },
          {
            "expr": "warehouse_compliance_checks_failed",
            "legendFormat": "Failed",
            "refId": "B"
          },
          {
            "expr": "warehouse_compliance_checks_pending",
            "legendFormat": "Pending",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 4,
          "w": 24,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "PPE Compliance Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "warehouse_ppe_compliance_percent",
            "legendFormat": "PPE Compliance %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 80
                },
                {
                  "color": "green",
                  "value": 95
                }
              ]
            },
            "unit": "percent"
          }
        },
        "gridPos": {
          "h": 4,
          "w": 8,
          "x": 0,
          "y": 12
        }
      },
      {
        "id": 5,
        "title": "Training Completion Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "warehouse_training_completion_percent",
            "legendFormat": "Training Completion %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 70
                },
                {
                  "color": "green",
                  "value": 90
                }
              ]
            },
            "unit": "percent"
          }
        },
        "gridPos": {
          "h": 4,
          "w": 8,
          "x": 8,
          "y": 12
        }
      },
      {
        "id": 6,
        "title": "Near Miss Events",
        "type": "stat",
        "targets": [
          {
            "expr": "increase(warehouse_near_miss_events_total[24h])",
            "legendFormat": "Near Misses (24h)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 3
                },
                {
                  "color": "red",
                  "value": 5
                }
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 4,
          "w": 8,
          "x": 16,
          "y": 12
        }
      },
      {
        "id": 7,
        "title": "Environmental Conditions",
        "type": "graph",
        "targets": [
          {
            "expr": "warehouse_temperature_celsius",
            "legendFormat": "Temperature (Â°C)",
            "refId": "A"
          },
          {
            "expr": "warehouse_humidity_percent",
            "legendFormat": "Humidity (%)",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Value",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 8,
        "title": "Emergency Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(warehouse_emergency_response_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(warehouse_emergency_response_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 9,
        "title": "Safety Violations by Category",
        "type": "piechart",
        "targets": [
          {
            "expr": "warehouse_safety_violations_by_category",
            "legendFormat": "{{category}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 24
        }
      }
    ],
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "refresh": "30s"
  }
}
