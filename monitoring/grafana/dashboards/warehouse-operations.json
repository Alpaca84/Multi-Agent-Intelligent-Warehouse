{
  "dashboard": {
    "id": null,
    "title": "Warehouse Operations - Detailed View",
    "tags": ["warehouse", "operations", "detailed"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Task Completion Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(warehouse_tasks_completed_total[5m]) / rate(warehouse_tasks_created_total[5m]) * 100",
            "legendFormat": "Completion Rate %",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Percentage",
            "min": 0,
            "max": 100
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Task Status Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "warehouse_tasks_by_status",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Worker Productivity",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(warehouse_tasks_completed_total{worker_id!=\"\"}[1h])",
            "legendFormat": "{{worker_id}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Tasks/hour",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Equipment Utilization",
        "type": "graph",
        "targets": [
          {
            "expr": "warehouse_equipment_utilization_percent",
            "legendFormat": "{{equipment_id}} - {{equipment_type}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Utilization %",
            "min": 0,
            "max": 100
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 5,
        "title": "Equipment Status",
        "type": "stat",
        "targets": [
          {
            "expr": "warehouse_equipment_status{status=\"operational\"}",
            "legendFormat": "Operational",
            "refId": "A"
          },
          {
            "expr": "warehouse_equipment_status{status=\"maintenance\"}",
            "legendFormat": "Maintenance",
            "refId": "B"
          },
          {
            "expr": "warehouse_equipment_status{status=\"offline\"}",
            "legendFormat": "Offline",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Order Processing Time",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(warehouse_order_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(warehouse_order_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 7,
        "title": "Inventory Turnover Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(warehouse_inventory_movements_total[1h])",
            "legendFormat": "{{movement_type}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Movements/hour",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 32
        }
      },
      {
        "id": 8,
        "title": "Pick Accuracy Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "warehouse_pick_accuracy_percent",
            "legendFormat": "Accuracy %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 90
                },
                {
                  "color": "green",
                  "value": 95
                }
              ]
            },
            "unit": "percent"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 32
        }
      }
    ],
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "refresh": "30s"
  }
}
